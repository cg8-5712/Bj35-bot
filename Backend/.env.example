# ======================================
# 数据库配置
# ======================================
DB_HOST=127.0.0.1
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=password
DB_POOL_MIN_SIZE=5
DB_POOL_MAX_SIZE=10
DB_SSL=false

# ======================================
# API 认证配置
# ======================================
 # 访问令牌
 accessToken=your_access_token_here

 # 访问令牌过期时间
 EXPIRE_TIME=your_access_token_expire_time_in_format_yyyy-MM-dd_HH:mm:ss+08:00

 # 访问密钥ID
 accessKeyId=your_access_key_id

 # 存储ID
 store_Id=your_store_id

 # 安全密钥
 SECRET_KEY=your_secret_key_here

 # JWT密钥 (用于用户认证)
 JWT_SECRET_KEY=your_jwt_secret_key_here_use_something_complex_and_random

# ======================================
# 企业微信 认证配置
# ======================================
# 企业ID
CORP_ID=your_corp_id_here

# 应用ID
AGENT_ID=your_agent_id_here

# 应用Secret
SECRET=your_secret_here